<ivy-module version="2.0">
    <info organisation="de.neyeon" module="feathry-dispatcher"/>
    <configurations defaultconfmapping="build->default;compile->compile(*),master(*);test,runtime->runtime(*),master(*)">
        <conf name="build"/>
        <conf name="compile"/>
        <conf name="test" extends="compile" />
        <conf name="runtime" extends="compile"/>
    </configurations>
    <dependencies>
        <dependency org="junit" name="junit" rev="4.7" />
        
        <dependency org="org.simpleframework" name="simple" rev="4.1.17" />
<dependency org="commons-beanutils" name="commons-beanutils" rev="1.8.2" />
<dependency org="com.thoughtworks.xstream" name="xstream" rev="1.3.1" />
<dependency org="aopalliance" name="aopalliance" rev="1.0" />
        
<!-- SLF4J -->
<dependency org="org.slf4j" name="slf4j-api" rev="1.5.10" />
<dependency org="org.slf4j" name="nlog4j" rev="1.2.25" />
<dependency org="commons-logging" name="commons-logging" rev="1.1.1" />

<!-- Spring -->
<dependency org="org.springframework" name="spring-core" rev="3.0.0.RELEASE" />
<dependency org="org.springframework" name="spring-beans" rev="3.0.0.RELEASE" />
<dependency org="org.springframework" name="spring-context" rev="3.0.0.RELEASE" />
<dependency org="org.springframework" name="spring-asm" rev="3.0.0.RELEASE" />
<dependency org="org.springframework" name="spring-test" rev="3.0.0.RELEASE" />
<dependency org="org.springframework" name="spring-expression" rev="3.0.0.RELEASE" />
<dependency org="org.springframework" name="spring-aop" rev="3.0.0.RELEASE" />
    </dependencies>
</ivy-module>
